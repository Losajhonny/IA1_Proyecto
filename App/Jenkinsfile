pipeline {
    agent any
    tools { nodejs "node16.2.0" }
    stages {
        stage ("build") {
            steps {
                dir("Instalar paquetes de node js") {
                    sh "npm install"
                }
            }
        }
        stage ("test") {
            steps {
                dir("Pruebas unitarias") {
                    sh "npm test"
                }
            }
        }

    }
}
