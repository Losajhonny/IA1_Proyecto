pipeline {
    agent any
    tools { nodejs "node16.2.0" }
    stages {
        stage ("build") {
            steps {
                sh "npm install"
            }
        }
        stage ("test") {
            steps {
                sh "npm run test"
            }
        }
        stage ("deploy") {
            steps {
                sh "npm start"
            }
        }
    }
}
